{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ]
  ],
  "plugins": [
    ["transform-react-jsx", {
      "pragma": "wp.element.createElement"
    }],
    "jsx-classnames"
  ],
  "env": {
    "default": {
      "plugins": ["transform-runtime", "jsx-classnames"]
    }
  }
}
